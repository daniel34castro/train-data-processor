# Ignore Python bytecode files
__pycache__/
*.pyc
*.pyo
*.pyd
*.pdb

# Ignore virtual environment folders
venv/
env/

# Ignore .env file for sensitive information (API keys, passwords, etc.)
.env

# Ignore Docker-related files
docker-compose.override.yml
.dockerignore

# Ignore log files
*.log

# Ignore configuration files generated by IDEs (e.g., VSCode, PyCharm)
.vscode/
.idea/

# Ignore coverage reports and tests' output
.coverage
nosetests.xml
coverage.xml
*.cover
*.hypothesis/

# Ignore built distributions (for example, when building a Python package)
*.egg
*.egg-info
dist/
build/

# Ignore any local test or build artifacts
*.bak
*.swp
*.swo

# Ignore other files that are specific to local development environments
*.sqlite3
*.db
*.db-journal
